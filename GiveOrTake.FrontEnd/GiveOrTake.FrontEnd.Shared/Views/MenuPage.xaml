<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GiveOrTake.FrontEnd.Shared.Views.MenuPage">
	<ContentPage.Content>
		<Grid>
			<!--<Grid.Margin>
				<OnPlatform x:TypeArguments="Thickness">
					<OnPlatform.Android>0, 20, 0, 0</OnPlatform.Android>
					<OnPlatform.iOS>0</OnPlatform.iOS>
					<OnPlatform.WinPhone>0</OnPlatform.WinPhone>
				</OnPlatform>
			</Grid.Margin>-->
			<StackLayout>
				<ListView ItemsSource="{Binding MenuItems}">
					<ListView.ItemTemplate>
						<DataTemplate>
							<ViewCell>
								<ViewCell.View>
									<Label Text="{Binding}"
										   FontSize="Medium"
										   Margin="8, 0, 0, 0"/>
								</ViewCell.View>
							</ViewCell>
						</DataTemplate>
					</ListView.ItemTemplate>
				</ListView>
			</StackLayout>
		</Grid>
	</ContentPage.Content>
</ContentPage>